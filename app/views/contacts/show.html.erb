<%= button_to "Delete", "/contacts/#{@contact.id}", method: :delete %>

<%= button_to "Edit", "/contacts/#{@contact.id}/edit", method: :get %>

<h1> Contact <%= params[:id] %></h1>

<h2><%= @contact.name %></h2>
<h2><%= @contact.email %></h2>
<h2><%= @contact.phone %></h2>
<h2><%= @contact.address %></h2>
<img src="<%= @contact.image %>">
<h2> Last Update: <%= @contact.updated_at %> </h2>